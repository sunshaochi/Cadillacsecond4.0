<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/title_act_layout"></include>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="24dp"
            android:paddingBottom="13dp"
            android:paddingTop="13dp"
            android:text="常见问题"
            android:textColor="#333333"
            android:textSize="13sp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1sp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:background="#eeeeee"></View>


        <TextView
            android:id="@+id/tv_gx"
            style="@style/tv_setcomtv"
            android:layout_height="40dp"
            android:background="@drawable/item_bg"
            android:drawableRight="@mipmap/right_jt"
            android:text="1    更新了什么" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1sp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:background="#eeeeee"></View>


        <TextView
            android:id="@+id/tv_tj"
            style="@style/tv_setcomtv"
            android:background="@drawable/item_bg"
            android:drawableRight="@mipmap/right_jt"
            android:text="2    怎么填写并提交数据" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1sp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:background="#eeeeee"></View>

        <TextView
            android:id="@+id/tv_zt"
            style="@style/tv_setcomtv"
            android:background="@drawable/item_bg"
            android:drawableRight="@mipmap/right_jt"
            android:text="3    怎么查看仅销售的数据提交状态" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1sp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:background="#eeeeee"></View>

        <TextView
            android:id="@+id/tv_sj"
            style="@style/tv_setcomtv"
            android:background="@drawable/item_bg"
            android:drawableRight="@mipmap/right_jt"
            android:text="4    怎么按照时间去查看历史数据" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1sp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:background="#eeeeee"></View>

        <TextView
            android:id="@+id/tv_xx"
            style="@style/tv_setcomtv"
            android:background="@drawable/item_bg"
            android:drawableRight="@mipmap/right_jt"
            android:text="5    怎么修改个人信息" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1sp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:background="#eeeeee"></View>

        <TextView
            android:id="@+id/tv_sp"
            style="@style/tv_setcomtv"
            android:background="@drawable/item_bg"
            android:drawableRight="@mipmap/right_jt"
            android:text="6    怎么进行新用户啊的审批" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1sp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:background="#eeeeee"></View>

    </LinearLayout>
</LinearLayout>