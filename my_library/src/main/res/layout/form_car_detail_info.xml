<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/colorWhite">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_agency_title"
            android:layout_width="@dimen/activity_detail_title_width"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/form_car_detail_info_table_head_paddingLeft"
            android:text="经销商"
            android:textColor="@color/colorDarkGreyLine"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_agency"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:text="无"
            android:textColor="@color/colorBlackText"
            android:textSize="20sp" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#000000" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_carType_title"
            android:layout_width="@dimen/activity_detail_title_width"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/form_car_detail_info_table_head_paddingLeft"
            android:text="车系"
            android:textColor="@color/colorDarkGreyLine"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_carType"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textColor="@color/colorBlackText"
            android:gravity="center_vertical"
            android:text="无"
            android:textSize="20sp" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#000000" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_configure_title"
            android:layout_width="@dimen/activity_detail_title_width"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/form_car_detail_info_table_head_paddingLeft"
            android:text="配置"
            android:textColor="@color/colorDarkGreyLine"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_configure"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textColor="@color/colorBlackText"
            android:gravity="center_vertical"
            android:text="无"
            android:textSize="20sp" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#000000" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_color_title"
            android:layout_width="@dimen/activity_detail_title_width"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/form_car_detail_info_table_head_paddingLeft"
            android:text="颜色"
            android:textColor="@color/colorDarkGreyLine"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_color"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textColor="@color/colorBlackText"
            android:gravity="center_vertical"
            android:text="无"
            android:textSize="20sp" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#000000" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_price_title"
            android:layout_width="@dimen/activity_detail_title_width"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/form_car_detail_info_table_head_paddingLeft"
            android:text="价格"
            android:textColor="@color/colorDarkGreyLine"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_price"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textColor="@color/colorBlackText"
            android:gravity="center_vertical"
            android:text="无"
            android:textSize="20sp" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#000000" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_addTime_title"
            android:layout_width="@dimen/activity_detail_title_width"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/form_car_detail_info_table_head_paddingLeft"
            android:text="入库日期"
            android:textColor="@color/colorDarkGreyLine"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_addTime"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:textColor="@color/colorBlackText"
            android:text="无"
            android:textSize="20sp" />


    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#000000" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_createTime_title"
            android:layout_width="@dimen/activity_detail_title_width"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/form_car_detail_info_table_head_paddingLeft"
            android:text="发布日期"
            android:textColor="@color/colorDarkGreyLine"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_createTime"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:textColor="@color/colorBlackText"
            android:text="无"
            android:textSize="20sp" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#000000" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_id_title"
            android:layout_width="@dimen/activity_detail_title_width"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/form_car_detail_info_table_head_paddingLeft"
            android:text="编号"
            android:textColor="@color/colorDarkGreyLine"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv_id"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:textColor="@color/colorBlackText"
            android:text="无"
            android:textSize="20sp" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#000000" />
</LinearLayout>